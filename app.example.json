{
  "expo": {
    "name": "quakey",
    "slug": "quakey",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/quakey_icon.png",
    "splash": {
      "image": "./assets/quakey-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.quakey.android",
      "versionCode": 3,
      "permissions": ["ACCESS_FINE_LOCATION"],
      "config": {
        "googleMaps": {
            "apiKey": "<android maps key>"
        }
      }
    }
  }
}
