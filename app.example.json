{
  "expo": {
    "name": "quakey",
    "slug": "quakey",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.9.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.quakey.android",
      "versionCode": 1
    }
  }
}
